{
  "entryPoints": [
    "../src-ext"
  ],
  "entryPointStrategy": "expand",
  "externalPattern": [
    "../src-ext/lib/**/*.ts"
  ],
  "exclude": [
    "../src-ext/test/**/*"
  ],
  "out": "extension",
  "name": "VS Code JsonSchema Builder",
  "entryDocument": "documentation.md",
  "includeVersion": true,
  "tsconfig": "../tsconfig-extension.json",
  "readme": "none",
  "sort": [
    "visibility",
    "static-first"
  ],
  "plugin": [
    "typedoc-plugin-markdown"
  ]
}