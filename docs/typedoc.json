{
  "entryPoints": [
    "../src-ext"
  ],
  "entryPointStrategy": "expand",
  "externalPattern": [
    "../src-ext/lib/**/*.ts"
  ],
  "exclude": [
    "../src-ext/test/**/*"
  ],
  "out": "extension",
  "includeVersion": true,
  "tsconfig": "../tsconfig-extension.json",
  "readme": "documentation.template.md",
  "sort": [
    "visibility",
    "static-first"
  ],
  "plugin": [
    "typedoc-umlclass"
  ],
  "umlClassDiagram": {
    "type": "detailed",
    "location": "local",
    "format": "svg"
  }
}