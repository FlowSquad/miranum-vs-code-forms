{
  "entryPoints": [
    "../src"
  ],
  "entryPointStrategy": "expand",
  "externalPattern": [
    "../src/utils/**/*.ts",
    "./types.ts"
  ],
  "exclude": [
    "../src/test/**/*"
  ],
  "out": "extension",
  "name": "VS Code JsonSchema Builder",
  "entryDocument": "documentation.md",
  "includeVersion": true,
  "tsconfig": "../tsconfig.json",
  "readme": "none",
  "sort": [
    "visibility",
    "static-first"
  ],
  "plugin": [
    "typedoc-plugin-markdown"
  ]
}
